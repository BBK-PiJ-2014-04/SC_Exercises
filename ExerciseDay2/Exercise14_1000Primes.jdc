println("Here's the list of the first 1000 primes");
int i = 1;
while(i < 1000) {
	boolean prime = true;
	if(i > 2) {
		for(int j = 2; j < i; j++) {
			if(i % j == 0) {
				prime = false;
				break;
			}
		}
	}
	if(prime || i < 3) {
		if(i == 1) {
			print(i);
		}
		else {
			print("," + i);
		}
		i++;
	}
}
