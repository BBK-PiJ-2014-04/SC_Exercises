println("Please enter your sentence"); 
input1 = System.console().readLine();
int howmany = 0;
String letterToFind = "e";

for(int i = 0; i < input1.length; i++) {
	if(input1.charAt(i).equals(letterToFind)) {
		howmany++;
	}		
}

print("There are " + howmany + " occurencies of " + letterToFind + " in the sentence you submitted");